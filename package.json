{
  "name": "karpi",
  "version": "1.2.1",
  "description": "Developer Productivity CLI - Unleash Your Potential",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "bin": {
    "karpi": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "build:binary": "bun build src/index.ts --compile --outfile dist/karpi",
    "bump:version": "bun scripts/bump-version.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "bun test",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "productivity",
    "developer-tools",
    "terminal"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.700.0",
    "@aws-sdk/client-rds": "^3.700.0",
    "@aws-sdk/credential-providers": "^3.700.0",
    "bcryptjs": "^2.4.3",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "clipboardy": "^5.0.1",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "date-fns": "^3.0.0",
    "gradient-string": "^2.0.2",
    "ink": "^4.4.1",
    "inquirer": "^9.2.12",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.4",
    "ora": "^7.0.1",
    "react": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bun": "latest",
    "@types/gradient-string": "^1.1.5",
    "@types/inquirer": "^9.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
